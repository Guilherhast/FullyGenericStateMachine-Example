# Makefile Commander

## Import config
ROOTDIR=$(realpath ../../../../)
ifndef CONFIG_SOURCED

CFG_MAKE=$(ROOTDIR)/Config.Makefile
include $(CFG_MAKE)

endif

## Module files
SRC_BASE=gate.body
MODULE_DIRECTORY=$(MODULE_INSTANCES_STATEMACHINE)/Gate/Body

## Side module info
HAS_SIDEMOD=false

## Dependencies
DEP_STATEMACHINE=$(MODOBJ_STATEMACHINE) $(MODOBJ_LIST)
DEP_GATE_TRANSITIONS=$(DEP_STATEMACHINE) $(MODOBJ_INS_GATE_TRANSITION)
DEP_GATE=$(DEP_GATE_TRANSITIONS) $(MODOBJ_INS_GATE_CONDITION) $(MODOBJ_INS_GATE_STATE)
DEP_MOD=$(MODOBJ_TESTUTILS) $(DEP_GATE) $(MODOBJ_INS_GATE_CONDITION) $(MODOBJ_STRUTILS)

# System
include $(RULES_MAKE)
