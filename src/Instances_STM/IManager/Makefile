# Makefile Commander Instance

## Import config
ROOTDIR=$(realpath ../../../)
ifndef CONFIG_SOURCED

CFG_MAKE=$(ROOTDIR)/Config.Makefile
include $(CFG_MAKE)

endif

## Module files
SRC_BASE=instancesManager
MODULE_DIRECTORY=$(SUBMODULE_INSTANCES_MANAGER_IMANAGER)

## Side module info
HAS_SIDEMOD=false

## Dependencies
DEP_STATEMACHINE_FULL=$(MODOBJ_STATEMACHINE) $(MODOBJ_LIST)
DEP_UTILS=$(MODOBJ_STRUTILS) $(MODOBJ_TESTUTILS)
DEP_COMMANDER=$(MODOBJ_COMMANDER_OBJ) $(DEP_STATEMACHINE_FULL) $(DEP_UTILS)
DEP_COMMANDER_INST=$(MODOBJ_INSTANCES_MANAGER_COMMANDER) $(DEP_COMMANDER) $(MODOBJ_COMMANDER_LIST)
DEP_MOD=$(DEP_COMMANDER_INST) $(MODOBJ_IOMANAGER)

# System
include $(RULES_MAKE)
